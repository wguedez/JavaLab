package Ejercicio1;

/**
 *
 * @author wguedez
 */
public class CalcularSueldo {
    
    public static void main(String args[]){
    
        Empleado empleado = new Empleado("Rosa gonzales", "11111", 40, 150);
        
        
        System.out.println("El sueldo del empleado: "+empleado.getNombreEmpleado()+", es el siguiente: "+empleado.calcularSueldo());
        
        
    }
    
}
