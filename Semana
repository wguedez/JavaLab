package Ejercicio1;

/**
 *
 * @author wguedez
 */
public class Semana {
    enum diasSemana {lunes, martes, miercoles, jueves,viernes,sabado,domingo};
    
    public static void main(String args[]){
        for(diasSemana dias:diasSemana.values()){
            System.out.println(dias);
        }
    }
}
